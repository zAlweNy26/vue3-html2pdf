{
  "name": "vue3-html2pdf",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zAlweNy26/vue3-html2pdf.git"
  },
  "files": ["dist"],
  "main": "./dist/vue3-html2pdf.es.js",
  "module": "./dist/vue3-html2pdf.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-html2pdf.es.js",
      "require": "./dist/vue3-html2pdf.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest --run",
    "build-only": "vue-tsc -d --emitDeclarationOnly && vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint ./src --ext .vue,.ts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "html2pdf.js": "^0.10.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "typescript": "~5.4.5",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.30",
    "vue-tsc": "^2.0.22"
  }
}
